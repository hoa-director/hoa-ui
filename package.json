  "scripts": {
    "build-client-dev": "ng build",
    "build-client-watch": "ng build --watch",
    "build-client-prod": "ng build --configuration=production",
    "build-client-staging": "npm run clean && ng build --configuration=staging",
    "build": "npm run clean && npm run build-client-prod",
    "client-local": "ng serve",
    "client-staging": "ng serve --configuration=staging",
    "clean": "rm -fR dist/",
    "dev": "npm run build-client-dev && npm run watch",
    "lint-client": "npx tslint -c tslint.json -p src/tsconfig.app.json"
  },
